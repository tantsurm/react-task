{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "brace-style": ["error", "stroustrup"],
        "react/prop-types": 0,
        "no-shadow": "off",
        "react/jsx-one-expression-per-line": "off",
        "camelcase": "off",
        "no-plusplus": "off"
    }
}